  <footer>
    <script type="text/javascript" src="/js/mapAPI.js"></script>    
    <script src="/js/handlebars.runtime.min.js"></script>
    <script>
        (function () {
            var revive    = Handlebars.template,
                templates = Handlebars.templates = Handlebars.templates || {};

          {{#templates}}
            templates['{{{name}}}'] = revive({{{template}}});
          {{/templates}}
        }());
